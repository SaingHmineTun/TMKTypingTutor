<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>

<BorderPane fx:id="root" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="800.0" stylesheets="@../css/light_theme.css" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.saimao.tmk_typing_tutor.controller.MainController">
   <center>
      <VBox prefHeight="285.0" prefWidth="716.0" spacing="15.0">
         <children>
            <HBox fx:id="hbSelection" alignment="CENTER_RIGHT" minHeight="50.0" spacing="10.0" BorderPane.alignment="CENTER">
               <children>
                  <VBox HBox.hgrow="ALWAYS">
                     <children>
                        <ComboBox fx:id="cbKeyboard" maxHeight="1.7976931348623157E308" prefWidth="200.0" promptText="KEYBOARD" VBox.vgrow="ALWAYS">
                           <effect>
                              <Lighting diffuseConstant="1.2">
                                 <bumpInput>
                                    <Shadow />
                                 </bumpInput>
                                 <light>
                                    <Light.Distant />
                                 </light>
                              </Lighting>
                           </effect>
                        </ComboBox>
                     </children>
                  </VBox>
                  <ComboBox fx:id="cbTheme" layoutX="267.0" layoutY="10.0" maxHeight="1.7976931348623157E308" prefWidth="150.0" promptText="THEME">
                     <effect>
                        <Lighting diffuseConstant="1.2">
                           <bumpInput>
                              <Shadow />
                           </bumpInput>
                           <light>
                              <Light.Distant />
                           </light>
                        </Lighting>
                     </effect>
                  </ComboBox>
                  <ComboBox fx:id="cbLevel" maxHeight="1.7976931348623157E308" prefWidth="150.0" promptText="MODE" HBox.hgrow="ALWAYS">
                     <effect>
                        <Lighting diffuseConstant="1.2">
                           <bumpInput>
                              <Shadow />
                           </bumpInput>
                           <light>
                              <Light.Distant />
                           </light>
                        </Lighting>
                     </effect>
                  </ComboBox>
                  <Button fx:id="btPrev" contentDisplay="GRAPHIC_ONLY" maxHeight="50.0" maxWidth="50.0" minHeight="50.0" minWidth="50.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="50.0" text="Prev">
                     <graphic>
                        <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../images/prev_dark.png" />
                           </image>
                        </ImageView>
                     </graphic>
                     <effect>
                        <Lighting diffuseConstant="1.2">
                           <bumpInput>
                              <Shadow />
                           </bumpInput>
                           <light>
                              <Light.Distant />
                           </light>
                        </Lighting>
                     </effect>
                  </Button>
                  <ComboBox id="lessons" fx:id="cbLessons" maxHeight="1.7976931348623157E308" prefWidth="200.0" promptText="LESSONS" HBox.hgrow="ALWAYS">
                     <effect>
                        <Lighting diffuseConstant="1.2">
                           <bumpInput>
                              <Shadow />
                           </bumpInput>
                           <light>
                              <Light.Distant />
                           </light>
                        </Lighting>
                     </effect></ComboBox>
                  <Button fx:id="btNext" contentDisplay="GRAPHIC_ONLY" maxHeight="50.0" maxWidth="50.0" minHeight="50.0" minWidth="50.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="50.0" text="Next">
                     <graphic>
                        <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../images/next_dark.png" />
                           </image>
                        </ImageView>
                     </graphic>
                     <effect>
                        <Lighting diffuseConstant="1.2">
                           <bumpInput>
                              <Shadow />
                           </bumpInput>
                           <light>
                              <Light.Distant />
                           </light>
                        </Lighting>
                     </effect>
                  </Button>
                  <Button fx:id="btSetting" contentDisplay="GRAPHIC_ONLY" layoutX="699.0" layoutY="10.0" maxHeight="50.0" maxWidth="50.0" minHeight="50.0" minWidth="50.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="50.0" text="Setting">
                     <graphic>
                        <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../images/setting_dark.png" />
                           </image>
                        </ImageView>
                     </graphic>
                     <effect>
                        <Lighting diffuseConstant="1.2">
                           <bumpInput>
                              <Shadow />
                           </bumpInput>
                           <light>
                              <Light.Distant />
                           </light>
                        </Lighting>
                     </effect>
                  </Button>
               </children>
            </HBox>
            <TextField id="view" fx:id="tfView" editable="false" focusTraversable="false" layoutX="10.0" layoutY="26.0" minHeight="-Infinity" prefHeight="50.0">
               <effect>
                  <Lighting diffuseConstant="2.0" specularConstant="0.23" specularExponent="10.92" surfaceScale="0.61">
                     <bumpInput>
                        <Shadow />
                     </bumpInput>
                     <light>
                        <Light.Distant>
                           <color>
                              <Color red="0.7236841917037964" green="0.7236841917037964" blue="0.7236841917037964" />
                           </color>
                        </Light.Distant>
                     </light>
                  </Lighting>
               </effect></TextField>
            <TextField id="practice" fx:id="tfPractice" minHeight="-Infinity" prefHeight="50.0" translateZ="100" />
             <HBox alignment="BOTTOM_CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
               <ImageView fx:id="ivLeftHand" fitHeight="250.0" fitWidth="250.0" pickOnBounds="true" preserveRatio="true">
                  <image>
                     <Image url="@../images/left_hand.png" />
                  </image></ImageView>
                 <VBox fx:id="vbKeyboardView" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="5.0" HBox.hgrow="ALWAYS" VBox.vgrow="ALWAYS">
                     <HBox fx:id="row1" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="5.0" VBox.vgrow="ALWAYS" />
                     <HBox fx:id="row2" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="5.0" VBox.vgrow="ALWAYS" />
                     <HBox fx:id="row3" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="5.0" VBox.vgrow="ALWAYS" />
                     <HBox fx:id="row4" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="5.0" VBox.vgrow="ALWAYS" />
                     <HBox fx:id="row5" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="5.0" VBox.vgrow="ALWAYS" />
                 </VBox>
               <ImageView fx:id="ivRightHand" fitHeight="250.0" fitWidth="250.0" layoutX="10.0" layoutY="10.0" pickOnBounds="true" preserveRatio="true">
                  <image>
                     <Image url="@../images/right_hand.png" />
                  </image>
               </ImageView>
             </HBox>
         </children>
         <padding>
            <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
         </padding>
      </VBox>
   </center>
